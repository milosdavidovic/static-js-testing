{
  "env": {
    "browser": true
  },
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": "eslint:recommended",
  "ignorePatterns": ["dist"],
  "rules": {
      "semi": "error",
      "quotes": ["error", "double"],
      "no-console": "warn",
      "prefer-template": "error"
  }
}